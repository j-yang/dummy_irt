{
  "name": "Dummy_IRT_for_Biopharma_Studies",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:pwa": "vite --config vite.pwa.config.ts",
    "dev:standalone": "vite --config vite.standalone.config.ts",
    "build": "vue-tsc && vite build",
    "build:pwa": "vue-tsc && vite build --config vite.pwa.config.ts",
    "build:standalone": "vue-tsc && vite build --config vite.standalone.config.ts",
    "build:all": "npm run build:pwa && npm run build:standalone",
    "preview": "vite preview",
    "preview:pwa": "vite preview --config vite.pwa.config.ts --outDir dist-pwa",
    "preview:standalone": "vite preview --config vite.standalone.config.ts --outDir dist-standalone",
    "deploy": "gh-pages -d dist",
    "deploy:pwa": "gh-pages -d dist-pwa",
    "deploy:standalone": "gh-pages -d dist-standalone",
    "deploy:irt": "gh-pages -d dist-pwa -r https://github.com/azu-oncology-rd/dummy_irt.git",
    "release:standalone": "npm run build:standalone && node scripts/release.js"
  },
  "dependencies": {
    "@vue-flow/background": "^1.3.2",
    "@vue-flow/controls": "^1.1.2",
    "@vue-flow/core": "^1.45.0",
    "@vue-flow/minimap": "^1.5.3",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jszip": "^3.10.1",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "gh-pages": "^6.3.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-pwa": "^1.0.1",
    "vite-plugin-singlefile": "^2.3.0",
    "vue-tsc": "^3.0.1",
    "workbox-window": "^7.3.0"
  }
}
